<a href="/item/<%- item._id %>">
    <img id="item-img" src="<%- item.imgUrl[0] %>" alt="<%- item.name %> image">
</a>
<a href="/item/<%- item._id %>">
    <h4 id="item-name"><%- item.name %></h4>
</a>
<p id="item-short-info"><%- item.shortInfo %></p>
<div>
    <p>Collected by <%- item.collectedBy.length %> users</p>
    <a href="/item/<%- item._id %>/edit-story"><img src="" alt="heart button image"></a>
</div>